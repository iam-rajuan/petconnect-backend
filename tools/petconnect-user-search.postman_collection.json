{
  "info": {
    "name": "PetConnect User Search",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Search Users",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/users/search?query={{user_search}}",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "users",
            "search"
          ],
          "query": [
            {
              "key": "query",
              "value": "{{user_search}}"
            }
          ]
        },
        "auth": {
          "type": "bearer",
          "bearer": [
            {
              "key": "token",
              "value": "{{access_token}}",
              "type": "string"
            }
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000/api/v1"
    },
    {
      "key": "access_token",
      "value": ""
    },
    {
      "key": "user_search",
      "value": "jane"
    }
  ]
}
